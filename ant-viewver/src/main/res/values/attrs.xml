<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BadgeFab">
        <attr name="badgeTextColor" format="color" />
        <attr name="badgeBackgroundColor" format="color" />
        <attr name="badgePosition" format="enum">
            <enum name="RightTop" value="0" />
            <enum name="LeftBottom" value="1" />
            <enum name="LeftTop" value="2" />
            <enum name="RightBottom" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="AntRefreshLayout">
        <attr name="offsetTop" format="integer"/>
        <attr name="customBar" format="boolean"/>
    </declare-styleable>

</resources>